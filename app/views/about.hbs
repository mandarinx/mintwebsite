{{> menu}}

<div class="about">
    <div class="content">
        <h1>About {{content.companyname}}</h1>
        <picture>
            <source srcset="{{content.image.main}}" media="(min-width: 960px)">
            <source srcset="{{content.image.wide}}" media="(min-width: 700px) and (max-width: 960px)">
            <source srcset="{{content.image.medium}}" media="(min-width: 400px) and (max-width: 700px)">
            <img srcset="{{content.image.small}}" alt="{{image.alt}}">
        </picture>
    </div>
</div>

